{
	"restartable": "rs",
	"ignore": [".git", "node_modules/", "dist/", "coverage/"],
	"watch": ["src/"],
	"verbose": true,
	"execMap": {
		"ts": "node -r ts-node/register"
	},
	"events": {
		"restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
	},
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js,json,ts.html"
}
